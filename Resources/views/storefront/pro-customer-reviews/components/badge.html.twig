{% block pro_customer_reviews_badge %}{% apply spaceless %}
    {% set theme = theme ?: config('ZmbGoogleCustomerReviews.config.theme') %}

    <a data-pro-customer-reviews target="_blank" data-key="{{ config('ZmbGoogleCustomerReviews.config.apiKey') }}" data-id="{{ config('ZmbGoogleCustomerReviews.config.placeId') }}" data-load-lib="{{ config('ZmbGoogleCustomerReviews.config.loadLib') }}" class="theme-{{theme}}{% if class %} {{ class }}{% endif %}">
        {% block pro_customer_reviews_badge_content %}
            {% block pro_customer_reviews_badge_rating %}
                <div class="rating" role="meter" aria-label="{{ 'pro.customerReviews.meter-label' | trans }}" aria-valuenow="0" aria-valuemin="0" aria-valuemax="5">
                    {% block pro_customer_reviews_badge_rating_content %}
                        {% block pro_customer_reviews_badge_rating_current %}
                            <div class="current"></div>
                        {% endblock %}

                        {% block pro_customer_reviews_badge_rating_stars %}
                            <div class="stars">
                                {% block pro_customer_reviews_badge_rating_stars_content %}
                                    <div class="star-group">
                                        {% block pro_customer_reviews_badge_rating_full_star %}
                                            <svg class="full-star" width="24" height="24" xmlns="http://www.w3.org/2000/svg">
                                                <path d="M12 17.27 18.18 21l-1.64-7.03L22 9.24l-7.19-.61L12 2 9.19 8.63 2 9.24l5.46 4.73L5.82 21z" fill="#FBBC04" fill-rule="evenodd"/>
                                            </svg>
                                        {% endblock %}
                                    </div>

                                    <div class="star-group">
                                        {% block pro_customer_reviews_badge_rating_half_star %}
                                            <svg class="half-star" width="24" height="24" xmlns="http://www.w3.org/2000/svg">
                                                <path d="m22 9.24-7.19-.62L12 2 9.19 8.63 2 9.24l5.46 4.73L5.82 21 12 17.27 18.18 21l-1.63-7.03L22 9.24ZM12 15.4V6.1l1.71 4.04 4.38.38-3.32 2.88 1 4.28L12 15.4Z" fill="#FBBC04" fill-rule="evenodd"/>
                                            </svg>
                                        {% endblock %}
                                    </div>

                                    <div class="star-group">
                                        {% block pro_customer_reviews_badge_rating_empty_star %}
                                            <svg class="empty-star" width="24" height="24" xmlns="http://www.w3.org/2000/svg">
                                                <path d="m22 9.24-7.19-.62L12 2 9.19 8.63 2 9.24l5.46 4.73L5.82 21 12 17.27 18.18 21l-1.63-7.03L22 9.24ZM12 15.4l-3.76 2.27 1-4.28-3.32-2.88 4.38-.38L12 6.1l1.71 4.04 4.38.38-3.32 2.88 1 4.28L12 15.4Z" fill="#FBBC04" fill-rule="evenodd"/>
                                            </svg>
                                        {% endblock %}
                                    </div>
                                {% endblock %}
                            </div>
                        {% endblock %}
                    {% endblock %}
                </div>
            {% endblock %}

            {% block pro_customer_reviews_badge_label %}
                <div class="label">
                    {% block pro_customer_reviews_badge_label_content %}
                        {% block pro_customer_reviews_badge_label_logo %}
                            <img class="google-logo" width="10" alt="Google" src="data:image/webp;base64,">
                        {% endblock %}

                        {% block pro_customer_reviews_badge_label_text %}
                            <span class="text">{{ 'pro.customerReviews.label' | trans }}</span>
                        {% endblock %}
                    {% endblock %}
                </div>
            {% endblock %}
        {% endblock %}
    </a>
{% endapply %}{% endblock %}
